<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>logtxtvar</Title>
    <Author>Leo</Author>
    <Shortcut>logtxtvar</Shortcut>
    <Description>Code snippet for an console.log function</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
      <SnippetType>SurroundsWith</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
        <Literal>
            <ID>string</ID>
            <ToolTip>string</ToolTip>
            <Default>string</Default>
        </Literal>
        <Literal>
            <ID>variable</ID>
            <ToolTip>variable</ToolTip>
            <Default>variable</Default>
        </Literal>
    </Declarations>
    <Code Language="JavaScript">
        <![CDATA[console.log("$string$:"+ $variable$);$end$]]></Code>
  </Snippet>
</CodeSnippet>